<h3 class="ui header">{{t "forms.i-i-s-otdel-logistiki-путевой-лист-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.выдатьТоплива
      class=(if (v-get validationModel "выдатьТоплива" "isInvalid") "error")
      label=(t "forms.i-i-s-otdel-logistiki-путевой-лист-e.выдатьТоплива-caption")
      data-test-i-i-s-otdel-logistiki-путевой-лист-e-выдатьТоплива=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "выдатьТоплива" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.номер
      class=(if (v-get validationModel "номер" "isInvalid") "error")
      label=(t "forms.i-i-s-otdel-logistiki-путевой-лист-e.номер-caption")
      data-test-i-i-s-otdel-logistiki-путевой-лист-e-номер=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "номер" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.сопровожЛица
      class=(if (v-get validationModel "сопровожЛица" "isInvalid") "error")
      label=(t "forms.i-i-s-otdel-logistiki-путевой-лист-e.сопровожЛица-caption")
      data-test-i-i-s-otdel-logistiki-путевой-лист-e-сопровожЛица=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "сопровожЛица" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.гаражНомер
      class=(if (v-get validationModel "гаражНомер" "isInvalid") "error")
      label=(t "forms.i-i-s-otdel-logistiki-путевой-лист-e.гаражНомер-caption")
      data-test-i-i-s-otdel-logistiki-путевой-лист-e-гаражНомер=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "гаражНомер" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-otdel-logistiki-путевой-лист-e.рукОтдЛог-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.рукОтдЛог
      displayAttributeName="фИО"
      autocomplete=true
      relationName="рукОтдЛог"
      projection="РукОтдЛогL"
      title=(t "forms.i-i-s-otdel-logistiki-путевой-лист-e.рукОтдЛог-caption")
      readonly=readonly
      componentName="рукОтдЛогLookup"
      class=(if (v-get validationModel "рукОтдЛог" "isInvalid") "error")
      data-test-i-i-s-otdel-logistiki-путевой-лист-e-рукОтдЛог=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "рукОтдЛог" "message")}}
  </div>
</form>
